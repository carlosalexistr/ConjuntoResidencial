<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="conjuntoResidencial" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
    <class>com.conjuntoResidencial.model.Propietario</class>
    <class>com.conjuntoResidencial.model.Multa</class>
    <class>com.conjuntoResidencial.model.Recibo</class>
    <class>com.conjuntoResidencial.model.Tipo</class>
    <class>com.conjuntoResidencial.model.Pago</class>
    <class>com.conjuntoResidencial.model.Grupofamiliar</class>
    <class>com.conjuntoResidencial.model.Asamblea</class>
    <class>com.conjuntoResidencial.model.Detallerecibo</class>
    <class>com.conjuntoResidencial.model.Solicitud</class>
    <class>com.conjuntoResidencial.model.Consejo</class>
    <class>com.conjuntoResidencial.model.Servicio</class>
    <class>com.conjuntoResidencial.model.Parentesco</class>
    <class>com.conjuntoResidencial.model.Cargo</class>
    <class>com.conjuntoResidencial.model.Inquilino</class>
    <class>com.conjuntoResidencial.model.Vivienda</class>
    <class>com.conjuntoResidencial.model.Concepto</class>
    <class>com.conjuntoResidencial.model.Asistencia</class>
    <class>com.conjuntoResidencial.model.Persona</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/conjunto?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
